version: '3.1'

services:
  guacamole:
    container_name: guacamole09
    build:
      context: ./guacamole09
      dockerfile: Dockerfile
    ports:
      - 18080:8080


  nginx:
    container_name: nginx09
    image: nginx
    ports:
      - 10080:80
    volumes:
      - ./nginx/conf.d/nginx.conf:/etc/nginx/conf.d/default.conf
      - ./nginx/log/guacamole.access.log:/var/log/nginx/guacamole.access.log
